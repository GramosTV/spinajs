{
  "name": "@spinajs/validation",
  "version": "2.0.4",
  "description": "validation library for spinajs framework",
  "main": "lib/index.js",
  "private": false,
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "build": "npm run clean && npm run compile",
    "compile": "tsc -p tsconfig.build.json",
    "prepare": "npm run build",
    "clean": "",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/validation.git"
  },
  "keywords": [
    "spinajs",
    "validation",
    "schema"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/core)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/validation/issues"
  },
  "homepage": "https://github.com/spinajs/validation#readme",
  "dependencies": {
    "@spinajs/configuration": "^2.0.4",
    "@spinajs/di": "^2.0.4",
    "@spinajs/exceptions": "^2.0.4",
    "@spinajs/log": "^2.0.4",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "ajv-merge-patch": "^5.0.1",
    "lodash": "^4.17.21"
  },
  "gitHead": "8322fcf31ba1dcdb57495d2eb93a88d9a6dafb39"
}
