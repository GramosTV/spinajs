  "bugs": {
    "url": "https://github.com/spinajs/main/issues"
  },
  "homepage": "https://github.com/spinajs/main#readme",
  "devDependencies": {
    "@types/mssql": "^8.1.2",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1"
  },
  "dependencies": {
    "@spinajs/configuration": "^2.0.46",
    "@spinajs/di": "^2.0.46",
