  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/main/issues"
  },
  "homepage": "https://github.com/spinajs/main#readme",
  "devDependencies": {
    "@types/mssql": "^7.1.5"
  },
  "dependencies": {
    "@spinajs/configuration": "^1.2.71",
    "@spinajs/di": "^1.2.68",
    "@spinajs/orm": "^1.2.77",
    "mssql": "^8.0.2",
    "typescript-mix": "^3.1.3"
  }
