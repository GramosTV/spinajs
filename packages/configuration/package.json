{
  "name": "@spinajs/configuration",
  "version": "1.2.4",
  "description": "framework configuration module",
  "main": "lib/index.js",
  "private": false,
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "build": "tsc",
    "build-watch": "tsc -w",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "types": "lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/configuration.git"
  },
  "keywords": [
    "di",
    "container",
    "spinajs",
    "di"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/core)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/configuration/issues"
  },
  "homepage": "https://github.com/spinajs/configuration#readme",
  "dependencies": {
    "@spinajs/di": "^1.2.4",
    "@spinajs/exceptions": "^1.2.4",
    "@spinajs/internal-logger": "^1.2.4",
    "ajv": "^8.8.2",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "ajv-merge-patch": "^5.0.1",
    "glob": "^7.1.4",
    "lodash": "^4.17.14"
  },
  "gitHead": "bbd6883b0e9d71ba1f7024e6937e309ce5691fcb"
}
