  "dependencies": {
    "@spinajs/configuration-common": "^1.2.126",
    "@spinajs/di": "^1.2.126",
    "@spinajs/exceptions": "^1.2.126",
    "@spinajs/internal-logger": "^1.2.126",
    "ajv": "^8.11.0",
