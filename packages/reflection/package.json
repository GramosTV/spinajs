{
  "name": "@spinajs/reflection",
  "version": "2.0.1",
  "description": "SpinaJS reflection helpers",
  "main": "lib/index.js",
  "private": false,
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "build": "npm run clean && npm run compile",
    "compile": "tsc -p tsconfig.build.json",
    "clean": "",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/core)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/reflection/issues"
  },
  "homepage": "https://github.com/spinajs/reflection#readme",
  "dependencies": {
    "@spinajs/configuration": "^2.0.0",
    "@spinajs/di": "^2.0.0",
    "@spinajs/exceptions": "^2.0.0",
    "@spinajs/log": "^2.0.0",
    "lodash": "^4.17.21",
    "typescript": "^4.7.4",
    "typescript-mix": "^3.1.3"
  },
  "devDependencies": {
    "@types/node": "^18.0.4"
  },
  "gitHead": "b5006e8fe511860940e87103eb3f63e672491178"
}
