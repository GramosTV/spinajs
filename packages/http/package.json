{
  "name": "@spinajs/http",
  "version": "2.0.13",
  "description": "framework HTTP module base on express.js",
  "main": "lib/index.js",
  "private": false,
  "scripts": {
    "build": "npm run clean && npm run compile && cpx src/views/**/* lib/views && cpx src/static/**/* lib/static",
    "compile": "tsc -p tsconfig.build.json",
    "clean": "",
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src --fix",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "types": "lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/main.git"
  },
  "keywords": [
    "spinajs",
    "http"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/main)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/main/issues"
  },
  "homepage": "https://github.com/spinajs/main#readme",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-signature": "^1.0.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/formidable": "^2.0.1",
    "@types/luxon": "^2.3.1",
    "@types/node": "^16.11.11",
    "@types/pug": "^2.0.4",
    "@types/uuid": "^8.3.3"
  },
  "dependencies": {
    "@spinajs/configuration": "^2.0.12",
    "@spinajs/di": "^2.0.12",
    "@spinajs/exceptions": "^2.0.12",
    "@spinajs/internal-logger": "^2.0.12",
    "@spinajs/log": "^2.0.13",
    "@spinajs/reflection": "^2.0.13",
    "@spinajs/validation": "^2.0.13",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cookie-signature": "^1.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fast-csv": "4.3.2",
    "formidable": "^2.0.1",
    "glob": "^7.1.4",
    "helmet": "^3.23.0",
    "lodash": "^4.17.14",
    "luxon": "^2.3.0",
    "mime": "^2.4.6",
    "pug": "^3.0.0",
    "randomstring": "^1.1.5",
    "tempfile": "3.0.0",
    "uuid": "^8.3.2"
  },
  "gitHead": "8a3f58de2dc2d8d077186d767581dd53be1d40cb"
}
