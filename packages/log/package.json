{
  "name": "@spinajs/log",
  "version": "1.1.9",
  "description": "Log lib for all spinejs related libs",
  "main": "lib/index.js",
  "private": false,
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "build": "tsc",
    "build-watch": "tsc -w",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/log)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/log/issues"
  },
  "homepage": "https://github.com/spinajs/log#readme",
  "dependencies": {
    "@spinajs/configuration": "file:../configuration",
    "@spinajs/di": "file:../di",
    "@spinajs/exceptions": "file:../exceptions",
    "@spinajs/validation": "file:../validation",
    "colors": "^1.4.0",
    "glob": "^7.2.0",
    "glob-to-regexp": "^0.4.1",
    "lodash": "^4.17.14",
    "luxon": "^2.3.0",
    "node-schedule": "^2.1.0"
  }
}
