{
  "name": "@spinajs/log",
  "version": "2.0.14",
  "description": "Log lib for all spinejs related libs",
  "main": "lib/index.js",
  "private": false,
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "build": "npm run clean && npm run compile",
    "compile": "tsc -p tsconfig.build.json",
    "clean": "",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/log)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/log/issues"
  },
  "homepage": "https://github.com/spinajs/log#readme",
  "dependencies": {
    "@colors/colors": "^1.5.0",
    "@spinajs/configuration": "^2.0.12",
    "@spinajs/di": "^2.0.12",
    "@spinajs/exceptions": "^2.0.12",
    "@spinajs/log-common": "^2.0.12",
    "ajv": "^8.11.0",
    "chalk": "4.1.2",
    "glob": "^7.2.0",
    "glob-to-regexp": "^0.4.1",
    "lodash": "^4.17.14",
    "luxon": "^2.3.0",
    "node-schedule": "^2.1.0"
  },
  "devDependencies": {
    "@types/mock-fs": "^4.13.1",
    "@types/node-schedule": "^1.3.2",
    "mock-fs": "^5.1.2"
  },
  "gitHead": "7c1d1b2520b5417f96f52de512d7fb816d86d1b0"
}
