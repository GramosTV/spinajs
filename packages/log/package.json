    "ajv": "^8.8.2",
    "colors": "^1.4.0",
    "glob": "^7.2.0",
    "glob-to-regexp": "^0.4.1",
    "lodash": "^4.17.14",
    "luxon": "^2.3.0",
    "node-schedule": "^2.1.0"
