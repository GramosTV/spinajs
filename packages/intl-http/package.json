  },
  "homepage": "https://github.com/spinajs/main#readme",
  "dependencies": {
    "@spinajs/configuration": "^2.0.8",
    "@spinajs/di": "^2.0.8",
    "@spinajs/exceptions": "^2.0.8",
    "@spinajs/http": "^2.0.8",
    "@spinajs/intl": "^2.0.8",
    "@spinajs/log": "^2.0.8",
    "@spinajs/reflection": "^2.0.8"
  },
  "devDependencies": {
