{
  "name": "@spinajs/electron",
  "version": "2.0.3",
  "description": "spinajs extensions for electron",
  "license": "ISC",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "types": "lib",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "compile": "tsc -p tsconfig.build.json",
    "clean": "",
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src --fix",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/di.git"
  },
  "keywords": [
    "di",
    "container",
    "spinajs",
    "di"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/core)",
  "bugs": {
    "url": "https://github.com/spinajs/di/issues"
  },
  "homepage": "https://github.com/spinajs/di#readme",
  "devDependencies": {
    "electron": "^17.0.0"
  },
  "dependencies": {
    "@spinajs/configuration-common": "^2.0.3",
    "@spinajs/di": "^2.0.3"
  },
  "gitHead": "2a4c44007a48fb9d37bda391cb2bba57ead12be9"
}
