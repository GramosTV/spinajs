{
  "name": "@spinajs/fs",
  "version": "2.0.46",
  "description": "wrapper for file operations",
  "exports": "./lib/index.js",
  "type": "module",
  "private": false,
  "engines": {
    "node": ">=16.11"
  },
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "prepare": "npm run build",

    "build": "npm run clean && npm run compile",
    "compile": "tsc -b tsconfig.json",
    "rimraf": "./node_modules/rimraf/bin.js",
    "clean": "rimraf lib/ && rimraf tsconfig.tsbuildinfo",
    
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/exceptions.git"
  },
  "keywords": [
    "di",
    "container",
    "spinajs",
    "di"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/core)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/exceptions/issues"
  },
  "homepage": "https://github.com/spinajs/exceptions#readme",
  "gitHead": "002dc553b0ffffd72193d0121ac425a4083bc9ee",
  "dependencies": {
    "@spinajs/exceptions": "2.0.39",
    "archiver": "^5.3.1",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/luxon": "^3.2.0",
    "@types/uuid": "^9.0.0"
  }
}
